
select *,'${CodigoAgenda}' as AgendaCodigo,'codigoFilialCliente' as codigoFilialCliente, 'Agenda' as Tabela from agenda with (NOLOCK) where codigo_agenda = '${CodigoAgenda}' 
																					    
select *,'${CodigoAgenda}' as AgendaCodigo,'codigoFilialCliente' as codigoFilialCliente, 'Agenda_Pedido' as Tabela from agenda_pedido with (NOLOCK) where codigo_agenda = '${CodigoAgenda}'
																					   
select *,'${CodigoAgenda}' as AgendaCodigo,'codigoFilialCliente' as codigoFilialCliente, 'Agenda_Pedido_Item' as Tabela from agenda_pedido_item with (NOLOCK) where codigo_agenda_pedido in(select codigo_agenda_pedido from agenda_pedido  with (NOLOCK) where codigo_agenda = '${CodigoAgenda}')
																					   
select *,'${CodigoAgenda}' as AgendaCodigo,'codigoFilialCliente' as codigoFilialCliente, 'Agenda_XML' as Tabela from agenda_xml with (NOLOCK) where codigo_agenda_xml in (select codigo_agenda_xml from agenda_pedido with (NOLOCK)  where codigo_agenda = '${CodigoAgenda}')
																					    
select *,'${CodigoAgenda}' as AgendaCodigo,'codigoFilialCliente' as codigoFilialCliente, 'Agenda_XML_Item' as Tabela from agenda_xml_item  with (NOLOCK) where codigo_agenda_xml in (select codigo_agenda_xml from agenda_pedido  with (NOLOCK) where codigo_agenda = '${CodigoAgenda}')
																					  
select *,'${CodigoAgenda}' as AgendaCodigo,'codigoFilialCliente' as codigoFilialCliente, 'Agenda_Portaria' as Tabela from agenda_portaria  with (NOLOCK) where codigo_agenda = '${CodigoAgenda}'
