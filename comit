select * from agenda with (NOLOCK) where codigo_agenda = '${CodigoAgenda}' 

select * from agenda_pedido with (NOLOCK) where codigo_agenda = '${CodigoAgenda}'

select * from agenda_pedido_item with (NOLOCK) where codigo_agenda_pedido in(select codigo_agenda_pedido from agenda_pedido  with (NOLOCK) where codigo_agenda = '${CodigoAgenda}')

select * from agenda_xml with (NOLOCK) where codigo_agenda_xml in (select codigo_agenda_xml from agenda_pedido with (NOLOCK)  where codigo_agenda = '${CodigoAgenda}')

select * from agenda_xml_item  with (NOLOCK) where codigo_agenda_xml in (select codigo_agenda_xml from agenda_pedido  with (NOLOCK) where codigo_agenda = '${CodigoAgenda}')

select * from agenda_portaria  with (NOLOCK) where codigo_agenda = '${CodigoAgenda}'
