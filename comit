DECLARE @Codigo_Agenda decimal(18,0) = 1,
		@Pedido varchar(25) = '255',
		@FilialCliente varchar(25) = '145',
		@Processado  bit =1 ,
		@Numero_De_Tentativa int = 2


IF NOT EXISTS(SELECT 1 FROM TT_AGENDAMENTO_PAF WHERE Codigo_Agenda = @Codigo_Agenda and Pedido = @Pedido and FilialCliente = @FilialCliente)
BEGIN 

INSERT INTO TT_AGENDAMENTO_PAF(Codigo_Agenda, Pedido, FilialCliente, Processado, Numero_De_Tentativa)
VALUES(@Codigo_Agenda,@Pedido,@FilialCliente,@Processado,@Numero_De_Tentativa)
end
ELSE 
BEGIN
UPDATE TT_AGENDAMENTO_PAF SET Processado = @Processado ,Numero_De_Tentativa = @Numero_De_Tentativa  where Codigo_Agenda = @Codigo_Agenda and Pedido = @Pedido and FilialCliente = @FilialCliente
END
