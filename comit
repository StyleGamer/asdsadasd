    MERGE INTO PROP_COMPRAS AS target
    USING (SELECT '${CodigoPedido}' as PEDIDO,'${status}' AS VALOR_PROPRIEDADE, '00180' AS PROPRIEDADE
		   UNION
		   SELECT CONCAT(@PEDIDO,'A') as PEDIDO,@VALOR_PROPRIEDADE AS VALOR_PROPRIEDADE, '00180' AS PROPRIEDADE
		  ) AS source
    ON TARGET.PEDIDO = SOURCE.PEDIDO AND TARGET.PROPRIEDADE = SOURCE.PROPRIEDADE
    WHEN MATCHED THEN
        UPDATE SET
            target.VALOR_PROPRIEDADE = source.VALOR_PROPRIEDADE
 
    WHEN NOT MATCHED BY TARGET THEN
        INSERT (
             PROPRIEDADE				
			,PEDIDO					
			,ITEM_PROPRIEDADE			
			,VALOR_PROPRIEDADE		
			,DATA_PARA_TRANSFERENCIA	
        )
        VALUES (
             source.PROPRIEDADE			
            ,source.PEDIDO					
		    ,1	
		    ,source.VALOR_PROPRIEDADE		
		    ,GETDATE()
        );
